<script>
/* وضع ديسكتوب/موبايل للهيدر */
function setHeaderMode(){
  const isDesktop = window.matchMedia('(min-width: 990px)').matches;
  document.getElementById('desktopRight').style.display = isDesktop? 'flex':'none';
  document.getElementById('desktopLeft').style.display  = isDesktop? 'flex':'none';
}
setHeaderMode(); addEventListener('resize', setHeaderMode);

/* فتح/غلق القوائم */
function toggleMenu(id){
  const el = document.getElementById(id);
  el.style.display = (el.style.display==='block')?'none':'block';
}
document.addEventListener('click', (e)=>{
  if(!e.target.classList.contains('btnx')){
    ['pMenu','cMenu','pMenuD','cMenuD'].forEach(id=>{
      const el=document.getElementById(id); if(el) el.style.display='none';
    });
  }
});

/* تبديل اتجاه بسيط */
function toggleLang(){
  const html=document.documentElement;
  const ar = html.lang!=='en';
  html.lang = ar ? 'en' : 'ar';
  html.dir  = ar ? 'ltr' : 'rtl';
}

/* بحث داخلي بـ Google */
function siteSearch(){
  const q=document.getElementById('q').value.trim();
  const host=location.host + location.pathname.replace(/\/[^/]*$/,'/');
  // نستخدمドومين الريبو الحالي
  const url=`https://www.google.com/search?q=site:${location.host}+${encodeURIComponent(q)}`;
  window.open(url,'_blank');
}

/* صفحات الموقع المعروفة */
const PAGES=[
  {path:'index.html',title:'الصفحة الرئيسية'},
  {path:'sokhna.html',title:'العين السخنة'},
  {path:'zafarana.html',title:'مشاريع طريق الزعفرانة'},
  {path:'suez.html',title:'مشاريع طريق السويس'},
  {path:'north.html',title:'الساحل الشمالي'},
  {path:'Cairo.html',title:'القاهرة الجديدة'},
  {path:'Resale.html',title:'طلب ريسيل'}
];

/* اقتراح الروابط + إعادة توجيه ذكية */
(function(){
  const sug=document.getElementById('suggest');
  PAGES.forEach(p=>{
    const a=document.createElement('a');
    a.href=p.path; a.innerHTML=`<span>${p.title}</span><span class="badge">${p.path}</span>`;
    sug.appendChild(a);
  });

  // لو المستخدم دخل مسار قريب من مسار موجود، نوجّهه تلقائيًا
  const current = location.pathname.split('/').pop().toLowerCase();
  if(!current) return;

  const dist=(a,b)=>{
    const m=[...Array(a.length+1)].map(()=>Array(b.length+1).fill(0));
    for(let i=0;i<=a.length;i++) m[i][0]=i;
    for(let j=0;j<=b.length;j++) m[0][j]=j;
    for(let i=1;i<=a.length;i++) for(let j=1;j<=b.length;j++)
      m[i][j]=Math.min(m[i-1][j]+1,m[i][j-1]+1,m[i-1][j-1]+(a[i-1]==b[j-1]?0:1));
    return m[a.length][b.length];
  };

  let best=null,score=1e9;
  PAGES.forEach(p=>{
    const d=dist(current,p.path.toLowerCase());
    if(d<score){score=d;best=p;}
  });
  if(best && score<=4){
    const msg=document.getElementById('autoMsg');
    msg.textContent=`يبدو أنك تقصد: ${best.title} — سيتم تحويلك خلال ثانيتين…`;
    setTimeout(()=>location.href=best.path,2000);
  }
})();
</script>
