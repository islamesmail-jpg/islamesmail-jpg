.group--left{justify-content:flex-end}
.group--right{justify-content:flex-start}
.brand{display:flex;align-items:center;justify-content:center}
.brand img{height:54px}

/* Mobile: اللوجو يمين – على الشمال (المشاريع/التواصل/AR) */
@media(max-width:990px){
  .topbar__in{grid-template-columns:auto 1fr}
  .brand{order:2;justify-content:flex-end}
  .group--mobile{order:1;justify-content:flex-start}
}

/* Buttons + menus */
.btnx{border:1px solid var(--gold);color:var(--gold);background:transparent;border-radius:999px;padding:9px 14px;font-weight:800;text-decoration:none;cursor:pointer}
.btnx:hover{background:var(--gold);color:#111}
.dropdown{position:relative}
.menu{display:none;position:absolute;top:110%;right:0;background:#0f1627;border:1px solid #2a3958;border-radius:12px;min-width:230px;padding:6px;box-shadow:0 12px 30px rgba(0,0,0,.35)}
.menu a{display:block;padding:10px 12px;border-radius:10px;color:#e7eefb;text-decoration:none;font-weight:700}
.menu a:hover{background:#16233b}

/* === 404 body === */
.wrap{max-width:980px;margin:28px auto;padding:0 16px}
.card{background:var(--surface);border:1px solid var(--line);border-radius:16px;padding:22px}
h1{margin:0 0 6px;font-size:clamp(26px,5.6vw,42px);font-weight:800}
.lead{color:var(--gold2);font-weight:800}
.actions{display:flex;gap:10px;flex-wrap:wrap;margin-top:12px}
.search{display:flex;gap:8px;margin-top:14px}
.search input{flex:1;border:1px solid var(--line);border-radius:12px;background:#0b1323;color:#fff;padding:12px 14px;font-weight:700}
.list{display:grid;gap:10px;margin-top:14px}
.list a{display:flex;justify-content:space-between;align-items:center;background:#0b1323;border:1px solid var(--line);border-radius:12px;padding:10px 12px;text-decoration:none;color:#e7eefb;font-weight:700}
.list a:hover{border-color:var(--gold)}
.badge{border:1px solid var(--gold);color:var(--gold);border-radius:999px;padding:4px 10px;font-size:.85rem}
.redir{margin-top:10px;color:#9fb0c8}
</style>
